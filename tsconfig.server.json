{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "./dist",
      "noEmit": false,
      "jsx": "react-jsx",
      "allowJs": true,
      "resolveJsonModule": true
    },
    "include": ["server/**/*"],
    "exclude": ["node_modules"]
  }
